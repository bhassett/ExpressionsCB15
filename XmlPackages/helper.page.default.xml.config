			                  </div>
			                  <div class='clear-both height-12'></div>
			                  <div id='search-result-wrapper'>
				                  <div id='search-columns'>
					                  <div class='cols-postal-code'><xsl:value-of select="ise:StringResource('createaccount.aspx.83')" disable-output-escaping="yes" /></div>
					                  <div class='cols-city'><xsl:value-of select="ise:StringResource('createaccount.aspx.84')" disable-output-escaping="yes" /></div>
					                  <div class='cols-state-code'><xsl:value-of select="ise:StringResource('address.cs.16')" disable-output-escaping="yes" /></div>
					                  <div class='cols-spare'> </div>
				                  </div>
				                  <div class='clear-both'></div>
				                  <div id='search-results'></div>
			                  </div>
			                  <div id='search-result-footer'>
				                  <div id='search-pages'>Page 0 of 0</div>
				                  <div id='records-found'>0 <xsl:value-of select="ise:StringResource('createaccount.aspx.98')" disable-output-escaping="yes" /></div>
			                  </div>
			                  <div class='clear-both'></div>
			                  <div id='search-result-pagination'></div>
		                  </div>
		                  <input type='hidden' id='address-section' value='Billing'/>
		                  <div id='records-found-label' class='hidden'>
			                  <xsl:value-of select="ise:StringResource('createaccount.aspx.98')" disable-output-escaping="yes" />
		                  </div>
                     </div>
                  
              </xsl:when>
              <xsl:otherwise>
                <div style='display:none;margin:auto' title='Address Verification' id='ise-address-verification'>
                     
	                <div class='address-verification-dialog-wrapper'>
		                <div class='clear-both height-5'></div>
		                <xsl:value-of select="ise:StringResource('createaccount.aspx.114')" disable-output-escaping="yes" />
		                <div class='clear-both height-5'></div>
		                <xsl:value-of select="ise:StringResource('createaccount.aspx.115')" disable-output-escaping="yes" />
		                <div class='clear-both height-12'></div>
		                <div id='divEnteredAddress'>
			                <ul class='ul-list-no-style'>
				                <li style='font-weight:bold'>
					                <xsl:value-of select="ise:StringResource('selectaddress.aspx.14')" disable-output-escaping="yes" />
				                </li>
				                <li><div class='clear-both height-5'></div></li>
				                <li id='c-address'></li>
				                <li id='c-city-state-postal'></li>
				                <li>
					                <div class='clear-both height-5'></div>
					                <div class='clear-both height-12'></div>
					                <input id='select-entered-address' class='btn btn-primary' type='button' value='Select Address'/>
				                </li>
			                </ul>
			                <div class='clear-both height-12'></div>
		                </div>
		                <div id='divSuggestedAddress'>
			                <ul  class='ul-list-no-style'>
				                <li style='font-weight:bold'>
					                <xsl:value-of select="ise:StringResource('selectaddress.aspx.15')" disable-output-escaping="yes" />
