				                </li>
			                </ul>
			                <div class='clear-both height-12'></div>
		                </div>
		                <div class='clear-both height-5'></div>
	</div>
                </div>
              </xsl:otherwise>
            </xsl:choose>
          </xsl:when>
          
          <!--DISPLAY_ADWORDS_CONVERSION_TRACKING_SCRIPT-->
          <xsl:when test="$SectionType='DISPLAY_ADWORDS_CONVERSION_TRACKING_SCRIPT'">
            <script type='text/javascript'>
              <xsl:value-of select="concat('/* &lt;![CDATA[ */ ','')"></xsl:value-of>
              var google_conversion_id = <xsl:value-of select="FIELD/ConversionId" />;
              var google_conversion_language = '<xsl:value-of select="FIELD/ConversionLanguage" />';
              var google_conversion_format = '<xsl:value-of select="FIELD/ConversionFormat" />';
              var google_conversion_color = '<xsl:value-of select="FIELD/ConversionColor" />';
              var google_conversion_label = '<xsl:value-of select="FIELD/ConversionLabel" />';
              var google_conversion_value = <xsl:value-of select="FIELD/ConversionValue" />;
              <xsl:value-of select="concat('/* ]]> */  ','')"></xsl:value-of>
            </script>
            <script type='text/javascript' src='//www.googleadservices.com/pagead/conversion.js'></script>
            <noscript>
	            <div style='display:inline;'>
		            <img height='1' width='1' style='border-style:none;' alt='' src='//www.googleadservices.com/pagead/conversion/{FIELD/ConversionId}/?value={FIELD/ConversionValue}&amp;label={FIELD/ConversionLabel}&amp;guid=ON&amp;script=0'/> 
	            </div>
            </noscript>
          </xsl:when>
          
          <!--DISPLAY_BUY_SAFE_SEAL_SCRIPT-->
          <xsl:when test="$SectionType='DISPLAY_BUY_SAFE_SEAL_SCRIPT'">
            <script src='//seal.buysafe.com/private/rollover/rollover.js'></script>
            <span id='BuySafeSealSpan'></span>
            <script type='text/javascript'>
              if(window.buySAFE &amp;&amp; buySAFE.Loaded) {
                buySAFE.Hash = '<xsl:value-of select="FIELD/SealHash" />';
                WriteBuySafeSeal('BuySafeSealSpan', 'GuaranteedSeal');
              }
            </script>
          </xsl:when>
          
          <!--DISPLAY_BING_ADS_TRACKING_SCRIPT-->
          <xsl:when test="$SectionType='DISPLAY_BING_ADS_TRACKING_SCRIPT'">
            <script type='text/javascript'> if (!window.mstag) mstag = {loadTag : function(){},time : (new Date()).getTime()};</script>
            <script id='mstag_tops' type='text/javascript' src='//flex.atdmt.com/mstag/site/{FIELDS/AdsGUID}/mstag.js'></script>
            <script type='text/javascript'>
              var objMsTag = {dedup:'<xsl:value-of select="FIELD/AdsDedup" />',domainId:'<xsl:value-of select="FIELD/AdsDomainId" />',type:'<xsl:value-of select="FIELD/AdsType" />', actionid: '<xsl:value-of select="FIELD/AdsActionId" />'};
              <xsl:if test="FIELD/LoadTagAdditional != ''">
	              var objMsTagAdditional = {<xsl:value-of select="FIELD/LoadTagAdditional" />};
	              objMsTag = $.extend({}, objMsTag, objMsTagAdditional);
              </xsl:if>
              mstag.loadTag('analytics', objMsTag);
            </script>
            <noscript><iframe src="//flex.atdmt.com/mstag/tag/{FIELD/AdsGUID}/analytics.html?{FIELD/QueryString}" frameborder='0' scrolling='no' width='1' height='1' style='visibility:hidden;display:none'></iframe></noscript>
          </xsl:when>
              
          <!--DISPLAY_STATES_OPTIONS-->
          <xsl:when test="$SectionType='DISPLAY_BING_ADS_TRACKING_SCRIPT'">
            <xsl:for-each select="FIELD/ITEM">
              <option value="{current()/Value}">
                <xsl:value-of select="current()/Text" disable-output-escaping="yes" />
              </option>
            </xsl:for-each>
          </xsl:when>

          <!--DISPLAY_USERNAME-->
          <xsl:when test="$SectionType='DISPLAY_USERNAME'">
            <xsl:value-of select="ise:StringResource('skinbase.cs.1')" disable-output-escaping="yes" />
            <a class="username" href="account.aspx">
              <xsl:value-of select="FIELD/Username" disable-output-escaping="yes" />
            </a>
          </xsl:when>
          
            <!--DISPLAY_CURRENCY_LIST-->
          <xsl:when test="$SectionType='DISPLAY_CURRENCY_LIST'">
            <xsl:variable name="SelectedCurrencyCode" select="FIELD/SelectedCurrencyCode"></xsl:variable>
            <xsl:variable name="ElementID" select="FIELD/ElementID"></xsl:variable>
            <select id="{$ElementID}" name="{$ElementID}" onChange="{FIELD/OnChange}">
              
              <xsl:for-each select="FIELD/Currencies/Currency">
                <xsl:choose>
                  <xsl:when test="current()/CurrencyCode = $SelectedCurrencyCode">
                    <option value="{current()/CurrencyCode}" selected="selected">
                      <xsl:value-of select="concat(current()/CurrencyCode ,' ', '(', current()/CurrencyDescription, ')')" />
                    </option>
                  </xsl:when>
                  <xsl:otherwise>
                    <option value="{current()/CurrencyCode}">
                      <xsl:value-of select="concat(current()/CurrencyCode ,' ', '(', current()/CurrencyDescription, ')')" />
                    </option>
                  </xsl:otherwise>
                </xsl:choose>
              </xsl:for-each>
            </select>
          </xsl:when>



          <!--DISPLAY_CHECKOUT_STEP-->
          <xsl:when test="$SectionType='DISPLAY_CHECKOUT_STEP'">
            <xsl:variable name="Step" select="number(FIELD/Step)"></xsl:variable>
            <xsl:variable name="ShoppingCartUrl" select="FIELD/ShoppingCartUrl"></xsl:variable>
            <xsl:variable name="ShippingUrl" select="FIELD/ShippingUrl"></xsl:variable>
            <xsl:variable name="PaymentUrl" select="FIELD/PaymentUrl"></xsl:variable>
            
            <div class="checkout-steps-container">
              <ul>
                <li>
                  <!--Shopping Cart-->
                  <a href="{$ShoppingCartUrl}">
                    <div class="row">
                      <div class="col-lg-3 col-md-3 col-sm-2 col-xs-2">
                        <div class="checkout-steps-number checkout-steps-active">1</div>
                      </div>
                      <div class="col-lg-9 col-md-9 col-sm-9 col-xs-9">
                        <div class="checkout-steps-text">Shopping Cart</div>
                      </div>
                    </div>
                  </a>
                </li>
                <li>
                  <!--Shipping-->
                  <xsl:choose>
                    <xsl:when test="$Step >= 2">
                      <a href="{$ShippingUrl}">
                        <div class="row">
                          <div class="col-lg-3 col-md-3 col-sm-2 col-xs-2">
                            <div class="checkout-steps-number checkout-steps-active">2</div>
                          </div>
                          <div class="col-lg-9 col-md-9 col-sm-9 col-xs-9">
                            <div class="checkout-steps-text">Shipping Address</div>
                          </div>
                        </div>
                      </a>
                    </xsl:when>
                    <xsl:otherwise>
                      <a href="javascript:void(0)">
                        <div class="row">
                          <div class="col-lg-3 col-md-3 col-sm-2 col-xs-2">
                            <div class="checkout-steps-number checkout-steps-inactive">2</div>
                          </div>
                          <div class="col-lg-9 col-md-9 col-sm-9 col-xs-9">
                            <div class="checkout-steps-text">Shipping Address</div>
                          </div>
                        </div>
                      </a>
                    </xsl:otherwise>
                  </xsl:choose>
                </li>
                <li>
                  <xsl:choose>
                    <xsl:when test="$Step >= 3">
                      <a href="{$PaymentUrl}">
                        <div class="row">
                          <div class="col-lg-3 col-md-3 col-sm-2 col-xs-2">
                            <div class="checkout-steps-number checkout-steps-active">3</div>
                          </div>
                          <div class="col-lg-9 col-md-9 col-sm-9 col-xs-9">
                            <div class="checkout-steps-text">Billing &amp; Payment</div>
                          </div>
                        </div>
                      </a>
                    </xsl:when>
                    <xsl:otherwise>
                      <a href="javascript:void(0)">
                        <div class="row">
                          <div class="col-lg-3 col-md-3 col-sm-2 col-xs-2">
                            <div class="checkout-steps-number checkout-steps-inactive">3</div>
                          </div>
                          <div class="col-lg-9 col-md-9 col-sm-9 col-xs-9">
                            <div class="checkout-steps-text">Billing &amp; Payment</div>
                          </div>
                        </div>
                      </a>
                    </xsl:otherwise>
                  </xsl:choose>
                </li>
              </ul>
            </div>
            
          </xsl:when>
          
          <xsl:otherwise>

          </xsl:otherwise>
        </xsl:choose>
      </xsl:template>
    </xsl:stylesheet>
    
  </PackageTransform>
